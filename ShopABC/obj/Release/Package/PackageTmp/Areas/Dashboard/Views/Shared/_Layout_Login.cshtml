<!DOCTYPE html>
<html lang="vi">
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="refresh" content="60">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link rel="icon" href="data:," />
	<link rel="preconnect" href="https://cdn.jsdelivr.net/">
    <script src="https://cdn.jsdelivr.net/gh/kimbaorrr/cdn/tailwindcss-3.4.0.js"></script> 
</head>
<body>
    <div class="flex items-center justify-center h-screen bg-gray-100">
        <div class="px-8 py-6 mt-4 text-left bg-white rounded-2xl shadow-xl">
            <div class="flex justify-center">
                <img style="height: 80px;" class="mt-1"
                     src="data:image/webp;base64," />
            </div>
            <h2 class="text-2xl font-bold text-center mt-3">@ViewBag.Title</h2>
            @RenderBody()
            <p class="text-center text-gray-500 text-xs mt-2">
                &copy; @DateTime.Now.ToString("yyyy") Bảo IT. All right reserved
            </p>
        </div>
    </div>
    @if (ViewBag.ThongBao != null)
    {
        <div class="absolute max-w-md m-auto shadow-md inset-x-0 top-0 p-4 mb-4 text-sm text-yellow-700 bg-yellow-100 rounded-lg dark:bg-yellow-200 dark:text-yellow-800" role="alert">
            <span class="font-medium ml-3">Cảnh báo: </span>@ViewBag.ThongBao
        </div>

    }
    <script>window.history.replaceState && window.history.replaceState(null, null, window.location.href)</script>
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.3/dist/jquery.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery-validation@1.18.0/dist/jquery.validate.min.js"></script>
</body>
</html>
